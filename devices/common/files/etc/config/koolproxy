
config global
	option time_update '4'
	option koolproxy_port '0'
	option startup_delay '5'
	option koolproxy_ipv6 '0'
	option enabled '1'
	option koolproxy_rules 'easylistchina.txt fanboy.txt yhosts.txt kp.dat user.txt'
	option koolproxy_mode '3'
	option koolproxy_acl_default '1'

config rss_rule
	option load '1'
	option name 'anti-AD仅此规则即可'
	option url 'https://anti-ad.net/easylist.txt'
	
config acl_rule
	option ipaddr '192.168.168.110'
	option proxy_mode '2'
	option remarks '选要滤https的客户端'

